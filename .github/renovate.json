{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":disableDependencyDashboard"],
  "regexManagers": [
    {
      "fileMatch": ["^tox.ini$"],
      "matchStrings": ["(?<depName>.*?)==(?<currentValue>.*?)\\s"],
      "datasourceTemplate": "pypi"
    }
  ]
}
