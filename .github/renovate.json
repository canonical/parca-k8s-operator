{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":disableDependencyDashboard"],
  "regexManagers": [
    {
      "fileMatch": ["tox.ini"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>\\S+)\n\\s+(?<depName>.*?)==(?<currentValue>.*?)\\n"
      ]
    }
  ]
}
